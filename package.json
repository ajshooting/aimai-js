    "@types/unorm": "^1",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "fastest-levenshtein": "^1.0.16",
