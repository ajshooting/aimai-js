    "katakana"
  ],
  "author": "ajshooting",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ajshooting/aimai-js.git"
  },
  "homepage": "https://github.com/ajshooting/aimai-js#readme",
  "bugs": {
    "url": "https://github.com/ajshooting/aimai-js/issues"
  },
